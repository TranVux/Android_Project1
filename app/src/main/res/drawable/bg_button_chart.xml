<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <stroke
                android:width="1dp"
                android:color="#17A3FC" />
            <corners
                android:radius="38dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#17A3FC"/>
            <stroke
                android:width="2dp"
                android:color="#17A3FC" />
            <corners android:radius="38dp"/>
        </shape>
    </item>
</selector>